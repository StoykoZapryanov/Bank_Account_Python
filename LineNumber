
class LineNumbers:
    # Open the file
    with open('C:\\testPython.txt') as file:

        # Iterate through lines
        for i, line in enumerate(file):

            line_list = []
            # Choose every line in a file
            if i < 5:
                line_list.append(line)
                print(line_list)

        # Close the file when you're done
        file.close()
